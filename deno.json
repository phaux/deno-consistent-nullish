{
  "name": "@prosto/nullish",
  "version": "0.2.0",
  "license": "MIT",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true
  },
  "exports": "./index.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/testing": "jsr:@std/testing@^1.0.9"
  },
  "fmt": {
    "lineWidth": 100
  },
  "lint": {
    "plugins": ["./index.ts"],
    "rules": {
      "include": ["no-console"]
    }
  },
  "tasks": {
    "snapshot": "deno test -RW=. -- -u"
  }
}
